[Unit]
Description=Gunicorn instance to serve myproject
After=network.target

[Service]
User= {{ unix_user }}
Group={{ unix_group }}
WorkingDirectory={{ install_dir }}
ExecStart={{ install_dir }}/bin/gunicorn --bind={{ hostname }}:{{ server_port }} syncmanagerapi.wsgi:app

[Install]
WantedBy=multi-user.target
